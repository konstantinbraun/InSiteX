                                                                           DeleteCommand="INSERT INTO History_S_TariffScopes SELECT * FROM System_TariffScopes WHERE [SystemID] = @original_SystemID AND [TariffID] = @original_TariffID AND [TariffContractID] = @original_TariffContractID AND [TariffScopeID] = @original_TariffScopeID; 

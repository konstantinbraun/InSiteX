                                                                           UpdateCommand="INSERT INTO History_S_TariffScopes SELECT * FROM System_TariffScopes WHERE [SystemID] = @original_SystemID AND [TariffID] = @original_TariffID AND [TariffContractID] = @original_TariffContractID AND [TariffScopeID] = @original_TariffScopeID; 
                                                                           UPDATE [System_TariffScopes] SET [NameVisible] = (CASE WHEN @ScopeID=1 THEN 'West' ELSE (CASE WHEN @ScopeID=2 THEN 'Ost' ELSE (CASE WHEN @ScopeID=3 THEN 'Berlin' ELSE 'Kein Gebiet' END) END) END), [DescriptionShort] = @DescriptionShort, [EditFrom] = @UserName, [EditOn] = SYSDATETIME(), ScopeID = @ScopeID WHERE [SystemID] = @original_SystemID AND [TariffID] = @original_TariffID AND TariffContractID = @original_TariffContractID AND [TariffScopeID] = @original_TariffScopeID">
